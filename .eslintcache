div>\r\n            <p>The current style is : { props.selectedStyle }</p>\r\n        </div>\r\n    );\r\n};\r\n\r\nFunctionalComponent.defaultProps = {\r\n    string: PropTypes.string.isRequired,\r\n    function: PropTypes.func.isRequired,\r\n    selectedStyle : PropTypes.string.isRequired,\r\n};","C:\\ReactFundamentalsModules\\react_fundamentals\\src\\components\\concepts\\Hooks.js",["97"],"import React, { useState, useEffect } from 'react';\r\n\r\nconst Hooks = () => {\r\n    const [query, setQuery] = useState('');\r\n    const [results, setResults] = useState({});\r\n\r\n    const fetcher = () => {\r\n        fetch(`https://swapi.dev/api/people/${query}`)\r\n            .then(res => res.json())\r\n            .then(json => {\r\n                console.log(json)\r\n                setResults(json);\r\n            })    \r\n        };\r\n\r\n    return(\r\n        <div className=\"main\">\r\n            <div classname=\"mainDiv\">\r\n                <input value={query} onChange={e => setQuery(e.target.value)} placeholder=\"enter your sw character number\" />\r\n                <button onClick={() => fetcher()}>Click for Character!</button>\r\n                {results ? <h2>{results.name}</h2> : <div></div>}\r\n            </div>\r\n        </div>\r\n    );\r\n};\r\n\r\nconst Hooks2 = () => {\r\n    const [results, queryNum, setQueryNum] = useNumHook('');\r\n    const [clicks, setClicks] = useClicks(0);\r\n\r\n    return(\r\n        <div className=\"main\">\r\n            <div className=\"mainDiv\">\r\n                <h3>Enter a number below to see a number fact.</h3>\r\n                <input value={queryNum} onChange={e => setQueryNum(e.target.value)} placeholder=\"enter a number\" />\r\n                {results ? <h2>{results}</h2> : <div></div>}\r\n                <button onClick={() => setClicks(clicks + 1)}>Click to update document title</button>\r\n             </div>\r\n         </div>\r\n    );\r\n};\r\n\r\nconst useNumHook = (num) => {\r\n    const [queryNum, setQueryNum] = useState(num);\r\n    const [results, setResults] = useState('');\r\n\r\n    useEffect(() => {\r\n        if (queryNum !== ''){\r\n            fetch(`http://numbersapi.com/${queryNum}`)\r\n            .then(res => res.text())\r\n            .then(json => {\r\n                setResults(json);\r\n                console.log(json)\r\n            });\r\n        };\r\n    }, [queryNum]);\r\n\r\n    return [results, queryNum, setQueryNum];\r\n};\r\n\r\n    const useClicks = (initCount) => {\r\n        const [clicks, setClicks] = useState(initCount);\r\n\r\n        useEffect(() => {\r\n            document.title = `You have clicked ${clicks} times`;\r\n        }, [clicks])\r\n        return [clicks, setClicks]\r\n    }\r\n\r\nexport default Hooks2;","C:\\ReactFundamentalsModules\\react_fundamentals\\src\\components\\apps\\timer-apps\\TimePiecesApp.js",[],"C:\\ReactFundamentalsModules\\react_fundamentals\\src\\components\\apps\\timer-apps\\TimerApp.js",[],"C:\\ReactFundamentalsModules\\react_fundamentals\\src\\components\\apps\\timer-apps\\ClockApp.js",[],"C:\\ReactFundamentalsModules\\react_fundamentals\\src\\components\\apps\\timer-apps\\StopWatchApp.js",[],{"ruleId":"98","replacedBy":"99"},{"ruleId":"100","replacedBy":"101"},{"ruleId":"102","severity":1,"message":"103","line":11,"column":24,"nodeType":"104","endLine":11,"endColumn":96},{"ruleId":"102","severity":1,"message":"103","line":16,"column":24,"nodeType":"104","endLine":16,"endColumn":89},{"ruleId":"102","severity":1,"message":"103","line":21,"column":24,"nodeType":"104","endLine":21,"endColumn":92},{"ruleId":"102","severity":1,"message":"103","line":26,"column":24,"nodeType":"104","endLine":26,"endColumn":110},{"ruleId":"102","severity":1,"message":"103","line":31,"column":24,"nodeType":"104","endLine":31,"endColumn":106},{"ruleId":"102","severity":1,"message":"103","line":36,"column":24,"nodeType":"104","endLine":36,"endColumn":80},{"ruleId":"102","severity":1,"message":"103","line":41,"co